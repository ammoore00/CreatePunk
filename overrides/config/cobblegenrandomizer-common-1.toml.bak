
#General settings
[general]
	#Use config instead of datapack
	use_config = true

	#List settings
	#Syntax: ["modid:block|weight"]
	#Example: ["minecraft:stone|2","minecraft:dirt|1"]
	#Forge tags are supported
	[general.lists]
		#Cobble gen
		block_list_cobble = ["minecraft:cobblestone"]
		#Stone gen
		block_list_stone = ["minecraft:stone"]
		#Basalt gen
		block_list_basalt = ["minecraft:basalt"]

		#Custom settings
		[general.lists.custom]
			#Custom generators
			#Syntax: [gen]
			#Gen: [type, block, list]
			#Type: cobblestone, stone
			#Block: resource location of the block below the generated block
			#List: see List settings
			#Examples:
			#custom_generators = [
			#   ["cobblestone", "minecraft:diamond_block", ["minecraft:diamond_block"]],
			#   ["cobblestone", "minecraft:dirt", ["forge:dirt"]],
			#   ["cobblestone", "minecraft:white_wool", ["minecraft:wool"]]]
			custom_generators = [
				["stone", "minecraft:andesite", ["minecraft:andesite"]], 
				["stone", "minecraft:granite", ["minecraft:granite"]], 
				["stone", "minecraft:diorite", ["minecraft:diorite"]], 
				["stone", "minecraft:tuff", ["minecraft:tuff"]], 
				["stone", "minecraft:calcite", ["minecraft:calcite"]], 
				["stone", "minecraft:deepslate", ["minecraft:deepslate"]], 
				["stone", "twigs:rhyolite", ["twigs:rhyolite"]], 
				["stone", "twigs:schist", ["twigs:schist"]], 
				["stone", "verdure:slate", ["verdure:slate"]],  
				["stone", "create:limestone", ["create:limestone"]], 
				["stone", "quark:jasper", ["quark:jasper"]], 
				["stone", "quark:peridotite", ["quark:peridotite"]], 
				["cobblestone", "minecraft:deepslate", ["minecraft:cobbled_deepslate"]], 
				["cobblestone", "minecraft:cobbled_deepslate", ["minecraft:cobbled_deepslate"]]
			]

